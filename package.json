{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "productName": "Youtube Downloader and Player",
  "description": "example",
  "author": "joe",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/icons-material": "^5.3.0",
    "@mui/lab": "^5.0.0-alpha.65",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "child_process": "^1.0.2",
    "electron-is-dev": "^2.0.0",
    "ffmpeg-static": "^4.4.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.1.0",
    "get-audio-duration": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.9.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^1.0.1",
    "ytdl-core": "^4.11.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "build-electron": "mkdir build/src && robocopy electron build/electron /S & robocopy src build/src /S",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extends": null,
    "asar": false,
    "publish": {
      "provider": "github",
      "repo": "youtube downloader and player",
      "owner": "jjmaxxx"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^13.2.1",
    "electron-builder": "^23.1.0",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^6.0.0"
  }
}
